{
  "idm": {
    "notificationFactory": {
      "_id": "notificationFactory",
      "enabled": {
        "$bool": "&{openidm.notifications|false}"
      },
      "threadPool": {
        "maxPoolThreads": 2,
        "maxQueueSize": 20000,
        "steadyPoolThreads": 1,
        "threadKeepAlive": 60
      }
    }
  }
}
