{
  "authenticationModules": {
    "activedirectory": {
      "_id": "activedirectory",
      "_type": {
        "_id": "activedirectory",
        "collection": false,
        "name": "Active Directory"
      },
      "defaults": {
        "authenticationLevel": 0,
        "connectionHeartbeatInterval": 1,
        "connectionHeartbeatTimeUnit": "MINUTES",
        "openam-auth-ldap-connection-mode": "LDAPS",
        "operationTimeout": 0,
        "primaryLdapServer": [
          "opendj-frodo-dev.classic.com:2636"
        ],
        "profileAttributeMappings": [],
        "returnUserDN": true,
        "searchScope": "SUBTREE",
        "secondaryLdapServer": [],
        "stopLdapbindAfterInmemoryLockedEnabled": false,
        "trustAllServerCertificates": false,
        "userBindDN": "uid=am-identity-bind-account,ou=admins,ou=identities",
        "userProfileRetrievalAttribute": "uid",
        "userSearchAttributes": [
          "uid"
        ],
        "userSearchStartDN": [
          "ou=identities"
        ]
      }
    }
  }
}
