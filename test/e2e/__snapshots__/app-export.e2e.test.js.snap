        "source": "function javaIsEqual(old, other) {
  var JsonPatch = org.forgerock.json.JsonPatch;
  var JsonValue = org.forgerock.json.JsonValue;
  return JsonPatch.diff(JsonValue(old), JsonValue(other)).asList().size() === 0;
}

