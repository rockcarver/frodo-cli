    "1c0c73e8-2be1-41ce-b042-3c39694346b5": {
      "_id": "1c0c73e8-2be1-41ce-b042-3c39694346b5",
      "context": "AUTHENTICATION_TREE_DECISION_NODE",
      "createdBy": "null",
