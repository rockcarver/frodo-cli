    "80c3e733-ae51-4851-a01d-1cbf193c80e9": {
      "_id": "80c3e733-ae51-4851-a01d-1cbf193c80e9",
      "context": "AUTHENTICATION_TREE_DECISION_NODE",
      "createdBy": "null",
      "creationDate": 0,
      "default": false,
      "description": "Activates the admin's account",
      "language": "JAVASCRIPT",
