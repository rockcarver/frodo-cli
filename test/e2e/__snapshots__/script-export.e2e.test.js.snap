        "  // Copy attributes from transient state",
        "  var transientObjAttrs = nodeState.get(OBJECT_ATTRS);",
        "  var attrs = ['aliasList', 'givenName', 'password', 'sn'];",
        "  for (var i = 0; i < attrs.length; i++) {",
