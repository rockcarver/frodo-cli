        "",
        "function utcNow() {",
        "  return new Date().toISOString();",
        "}",
        "",
        "try {",
        "  ",
        "  var OBJECT_ATTRS = 'objectAttributes';",
        "",
        "  // Start by getting object attributes from shared state",
        "  var sharedObjAttrs = sharedState.get(OBJECT_ATTRS);",
