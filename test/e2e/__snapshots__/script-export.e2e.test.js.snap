{
  "meta": Any<Object>,
  "script": {
    "13cd3c60-a04b-4455-b028-fbfd01ed88b1": {
      "_id": "13cd3c60-a04b-4455-b028-fbfd01ed88b1",
      "context": "AUTHENTICATION_TREE_DECISION_NODE",
