        "    "               options:{\\n" +",
        "    "                   title:'" + onfido_dialog_title + "',\\n" +",
        "    "                   descriptions:[\\n" +",
        "    "                       '" + onfido_dialog_msg1 + "',\\n" +",
        "    "                       '" + onfido_dialog_msg2 + "',\\n" +",
        "    "                   ]\\n" +",
        "    "               }\\n" +",
        "    "          },\\n" +",
        "    "          'document',\\n" +",
        "    "          'face',\\n" +",
